{
    "name": "memory-viz",
    "version": "0.7.0",
    "description": "Library for creating beginner-friendly memory model diagrams.",
    "type": "module",
    "exports": {
        "types": "./dist/index.d.ts",
        "require": "./dist/memory-viz.node.cjs",
        "import": {
            "node": "./dist/memory-viz.node.js",
            "default": "./dist/memory-viz.browser.js"
        },
        "default": "./dist/memory-viz.umd.js"
    },
    "homepage": "https://www.cs.toronto.edu/~david/memory-viz",
    "scripts": {
        "test": "NODE_OPTIONS='--experimental-vm-modules' jest --no-cache",
        "test-cov": "NODE_OPTIONS='--experimental-vm-modules' jest --no-cache --coverage",
        "watch": "webpack --watch --config webpack.dev.js",
        "build-dev": "tsc && webpack --config webpack.dev.js",
        "build": "tsc && webpack --config webpack.prod.js",
        "prepare": "npm run build"
    },
    "keywords": [
        "education",
        "diagrams"
    ],
    "author": {
        "name": "David Liu",
        "email": "david@cs.toronto.edu",
        "url": "https://www.cs.toronto.edu/~david"
    },
    "contributors": [
        "Max Chen",
        "Mimis Chlympatsos",
        "Omid Hemmati",
        "Yoonie Jang",
        "Shannon Komguem",
        "Hoi Ching (Herena) Li",
        "Siqi (Leo) Liu",
        "Josh Shergill",
        "Utku Egemen Umut",
        "Sarah Wang",
        "Ziyuan (Jerry) Zhang"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/david-yz-liu/memory-viz.git",
        "directory": "memory-viz"
    },
    "bugs": {
        "url": "https://github.com/david-yz-liu/memory-viz/issues"
    },
    "bin": {
        "memory-viz": "dist/cli.js"
    },
    "dependencies": {
        "@xmldom/xmldom": "^0.8.11",
        "commander": "^14.0.0",
        "deepmerge": "^4.3.1",
        "i18next": "^23.15.0",
        "roughjs": "^4.6.6",
        "zod": "^4.0.5"
    },
    "devDependencies": {
        "@babel/core": "^7.28.5",
        "@babel/preset-env": "^7.28.5",
        "@babel/preset-typescript": "^7.28.5",
        "@types/jest": "^29.5.12",
        "@types/node": "^22.10.3",
        "@types/pretty": "^2.0.3",
        "@types/tmp": "^0.2.6",
        "babel-loader": "^10.0.0",
        "copy-webpack-plugin": "^12.0.2",
        "csstype": "^3.1.3",
        "execa": "5.1.1",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "node-html-parser": "^6.1.13",
        "pretty": "^2.0.0",
        "tmp": "^0.2.4",
        "ts-loader": "^9.5.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.0.1",
        "webpack-merge": "^6.0.1"
    }
}
